package Noxml;

import java.util.List;

public class Student {
	private String name;
	private int id;
	
	private Test t;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public List<String> getCourse() {
		return course;
	}

	public void setCourse(List<String> course) {
		this.course = course;
	}

	public Test getT() {
		return t;
	}

	public void setT(Test t) {
		this.t = t;
	}

	public Student(String name, int id, List<String> course, Test t) {
		super();
		this.name = name;
		this.id = id;
		this.course = course;
		this.t = t;
	}

	public Student() {
		super();
		// TODO Auto-generated constructor stub
	}

	@Override
	public String toString() {
		return "Student [name=" + name + ", id=" + id + ", course=" + course + ", t=" + t + "]";
	}
	
	
	
}
