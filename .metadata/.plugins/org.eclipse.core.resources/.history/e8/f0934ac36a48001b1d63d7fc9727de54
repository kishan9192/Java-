import java.util.*;
public class Arraylist_to_array {
	public static void main(String[] args) {
		List<Integer>l1 = new ArrayList<Integer>();
		l1.add(1);
		l1.add(2);
		l1.add(3);
		l1.add(4);
		
		Integer[] arr = new Integer[l1.size()];
		arr = l1.toArray(arr);
		for (int i:arr) {
			System.out.println(i);
		}
	}
}
