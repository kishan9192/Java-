package package2;
class Parent {
	public void show(String s) {
		System.out.print("Base" + s);
	}
}
class Child extends Parent {
	
	public void show(String s) {
		System.out.print("Child" + s);
	}
}
public class overriding {
	public static void main(String args[]) {
		Child obj = new Child();
		obj.show();
	}
	
}
