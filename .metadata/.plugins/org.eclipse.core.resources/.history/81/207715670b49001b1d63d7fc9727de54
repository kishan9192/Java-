package package2;
class Parent {
	public void show() {
		System.out.print("In base");
	}
}
class Child extends Parent {
	@Override
	public void show() {
		System.out.print("In child");
	}
}
public class overriding {
	public static void main(String args[]) {
		Child obj = new Child();
		obj.show();
	}
}
